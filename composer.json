{
	"name": "99tests/99tests",
	"description": "A framework to generate valuable test cases automatically*",
	"type": "library",
	"keywords": [
		"php",
		"tests",
		"generator",
		"test cases"
	],	
    "require": {
		"php" : "^7.0",
        "symfony/dotenv": "^4.3",
		"symfony/yaml": "^4.3",
		"symfony/console": "^4.3",
		"symfony/phpunit-bridge": "^4.3"
    },
	"require-dev": {
    	"phpunit/phpunit": "7.5.14"
  	},
	"autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
